{
  "name": "beatsaver-electron",
  "version": "1.0.0",
  "description": "An Electron app wrapping BeatSaver which adds duplicate song detection and other features",
  "main": "src/main.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "npm run build && electron ./dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JakeCover/BeatSaverElectron.git"
  },
  "keywords": [
    "beatsaber",
    "beatsaver",
    "scoresaver",
    "mods"
  ],
  "author": "Cobular",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JakeCover/BeatSaverElectron/issues"
  },
  "homepage": "https://github.com/JakeCover/BeatSaverElectron#readme",
  "devDependencies": {
    "electron": "^13.1.8",
    "prettier": "^2.3.2"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "arrowParens": "always"
  }
}
